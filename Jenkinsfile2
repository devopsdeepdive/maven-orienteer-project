pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing is competed'
            }
        }
         stage('Deploy') { 
            steps {
                echo 'Deployment is competed'
            }
        }
         stage('Result') {
            steps {
                echo 'Deployment results shared'
            }
        }
    }
    
}
